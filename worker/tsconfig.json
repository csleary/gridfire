{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021"],
    "module": "node16",
    "moduleResolution": "node16",
    "baseUrl": "./",
    "paths": {
      "gridfire-worker/*": ["*"]
    },
    "allowJs": true,
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
